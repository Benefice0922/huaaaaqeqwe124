    <!-- New clean payment template styles -->
    <style>
        :root{
          --bg: #ffffff;
          --bg-soft: #f6f7fb;
          --card: #ffffff;
          --text: #0f172a;
          --muted: #6b7280;
          --border: #e5e7eb;
          --primary: #0066cc;
          --primary-600: #0052a3;
          --success: #16a34a;
          --warning: #f59e0b;
          --warning-text: #b45309;
          --radius-xl: 16px;
          --radius-lg: 12px;
          --radius-md: 10px;
          --shadow-sm: 0 1px 2px rgba(0,0,0,.04);
          --shadow-md: 0 2px 8px rgba(0,0,0,.06);
          --gap: 16px;
        }

        /* Payment template section override */
        .payment-template-section {
          background: var(--card);
          border: 1px solid var(--border);
          border-radius: var(--radius-xl);
          box-shadow: var(--shadow-md);
          padding: 18px;
          margin: 24px 0;
          font-family: ui-sans-serif, system-ui, -apple-system, Segoe UI, Roboto, Inter, "Helvetica Neue", Arial;
          color: var(--text);
        }
        
        .payment-template-section .hero-head{
          display: grid; grid-template-columns: 1fr auto; align-items: center; gap: 12px;
        }

        /* Animated status */
        .payment-template-section .status{
          position: relative;
          display: inline-flex; align-items: center; gap: 8px; font-size: 12px; font-weight: 700;
          border-radius: 999px; padding: 6px 10px;
          color: var(--warning-text); background: #fffbeb; border: 1px solid #fde68a;
          animation: statusGlowWarn 4s ease-in-out infinite;
        }
        
        .payment-template-section .status .dot{
          position: relative; width: 8px; height: 8px; border-radius: 999px;
          background: var(--warning);
        }
        .payment-template-section .status .dot::after{
          content:""; position:absolute; inset:-6px; border-radius:999px;
          border: 2px solid rgba(245,158,11,.35);
          animation: rippleWarn 2.4s ease-out infinite;
        }

        .payment-template-section h2{ margin: 6px 0 2px; font-size: 20px; letter-spacing: .2px; color: var(--text); }
        .payment-template-section .sub{ margin: 0; color: var(--muted); font-size: 14px; }

        .payment-template-section .btn-primary{
          display: inline-flex; align-items: center; justify-content: center; gap: 10px;
          background: var(--primary); color: #fff; border: 1px solid var(--primary-600);
          border-radius: 12px; padding: 12px 16px; font-weight: 800; letter-spacing: .2px; cursor: pointer;
          box-shadow: 0 1px 0 rgba(255,255,255,.5) inset, 0 2px 8px rgba(0,102,204,.22);
          transition: filter .15s ease, transform .05s ease;
          text-decoration: none;
        }
        .payment-template-section .btn-primary:hover{ filter: brightness(1.06) }
        .payment-template-section .btn-primary:active{ transform: translateY(1px) }

        /* Meta */
        .payment-template-section .meta{
          display: grid; grid-template-columns: repeat(3, minmax(0,1fr)); gap: 10px; margin-top: 14px;
        }
        .payment-template-section .meta .item{
          border: 1px solid var(--border); border-radius: 10px; padding: 12px; background: #fff;
          display: grid; align-content: start; min-height: 76px;
        }
        .payment-template-section .meta .label{ color: var(--muted); font-size: 12px; margin-bottom: 4px; }
        .payment-template-section .meta .value{ font-weight: 700; word-break: break-word; }

        /* Tracker */
        .payment-template-section .tracker{
          margin-top: 14px; padding: 14px; border-radius: 12px; border: 1px solid var(--border); background: #fff;
        }
        .payment-template-section .steps{
          --cols: 4;
          display: grid; grid-template-columns: repeat(var(--cols), 1fr); gap: 12px;
        }
        .payment-template-section .step{
          display: grid; grid-template-columns: 18px 1fr; align-items: start; gap: 8px;
          padding: 10px 8px; border-radius: 10px; background: #fff; border: 1px solid #f3f4f6;
        }
        .payment-template-section .step .dot{
          position: relative; width: 12px; height: 12px; border-radius: 999px; border: 2px solid var(--border); background: #fff;
          transform-origin: center;
        }
        .payment-template-section .step.done .dot{
          border-color: var(--success); background: var(--success);
          animation: pulseDot 2.8s ease-in-out infinite;
        }
        .payment-template-section .step.done .dot::after{
          content:""; position:absolute; inset:-6px; border-radius:999px;
          border: 2px solid rgba(22,163,74,.35);
          animation: rippleSucc 2.8s ease-out infinite;
        }
        .payment-template-section .step.current .dot{
          border-color: var(--primary); background: #fff;
          box-shadow: 0 0 0 3px rgba(0,102,204,.18);
          animation: pulseDot 1.6s ease-in-out infinite;
        }
        .payment-template-section .step.current .dot::after{
          content:""; position:absolute; inset:-6px; border-radius:999px;
          border: 2px solid rgba(0,102,204,.35);
          animation: ripplePrim 1.8s ease-out infinite;
        }
        .payment-template-section .step .name{ font-weight: 700; font-size: 13px; }
        .payment-template-section .step .time{ color: var(--muted); font-size: 12px; margin-top: 2px; }

        /* Grid layout */
        .payment-template-section .grid{
          display: grid; grid-template-columns: 1.35fr .65fr; gap: var(--gap); margin-top: 14px;
          align-items: start;
        }

        /* Cards */
        .payment-template-section .card{
          background: var(--card);
          border: 1px solid var(--border);
          border-radius: var(--radius-lg);
          box-shadow: var(--shadow-sm);
          padding: 16px;
        }
        .payment-template-section .title-sm{ margin: 0 0 10px; font-size: 16px; letter-spacing: .2px; }

        /* Items */
        .payment-template-section .items{
          margin-top: 12px; border: 1px solid var(--border); border-radius: 12px; background: #fff;
          overflow: hidden;
        }
        .payment-template-section table{ width: 100%; border-collapse: collapse; font-size: 14px; table-layout: fixed; }
        .payment-template-section thead th{
          text-align: left; padding: 10px 12px; color: var(--muted); font-weight: 700; letter-spacing: .2px;
          border-bottom: 1px solid var(--border); background: #fafafa;
        }
        .payment-template-section tbody td{ padding: 12px; border-bottom: 1px solid var(--border); }
        .payment-template-section tbody tr:hover td{ background: #fcfcfc; }
        .payment-template-section .items table th:nth-child(4),
        .payment-template-section .items table td:nth-child(4),
        .payment-template-section .items table td.price{
          text-align: right; white-space: nowrap;
          font-variant-numeric: tabular-nums lining-nums; font-feature-settings: "tnum" 1, "lnum" 1;
        }

        /* Totals */
        .payment-template-section .totals{
          display: grid; grid-template-columns: 1fr auto; gap: 8px; margin-top: 12px; padding-top: 12px; border-top: 1px dashed var(--border);
        }
        .payment-template-section .totals .label{ color: var(--muted); }
        .payment-template-section .totals .val{ font-weight: 700; }
        .payment-template-section .totals .total{ font-size: 18px; }

        /* Side column */
        .payment-template-section .stack{ display: grid; gap: var(--gap); }
        .payment-template-section .field{
          display: grid; grid-template-columns: 110px 1fr; gap: 10px; padding: 10px 12px;
          border: 1px dashed var(--border); border-radius: 10px; align-items: center; background: #fff;
        }
        .payment-template-section .field .label{ color: var(--muted); font-size: 12px; }
        .payment-template-section .field .value{ font-weight: 700; word-break: break-word; }

        /* Support chat */
        .chatra--webkit {
          position: fixed;
          right: 20px;
          bottom: 20px;
          z-index: 2147483647;
        }
        .support-circle {
          position: fixed;
          right: 20px;
          bottom: 20px;
          width: 60px;
          height: 60px;
          background: var(--primary);
          border-radius: 50%;
          cursor: pointer;
          display: flex;
          align-items: center;
          justify-content: center;
          box-shadow: 0 4px 12px rgba(0,102,204,.3);
          z-index: 2147483646;
        }
        .support-circle::after {
          content: "üí¨";
          font-size: 24px;
        }

        /* Mobile responsive */
        @media (max-width: 960px){
          .payment-template-section .grid{ grid-template-columns: 1fr; }
          .payment-template-section .steps{ --cols: 2; }
        }
        @media (max-width: 640px){
          .payment-template-section .meta{ grid-template-columns: 1fr 1fr; }
          .payment-template-section .steps{ --cols: 1; }
          .payment-template-section .btn-primary{ width: 100%; padding: 14px; }
          .payment-template-section{ padding: 20px 16px; }
        }
        @media (max-width: 420px){
          .payment-template-section .meta{ grid-template-columns: 1fr; }
        }

        /* Table -> cards on narrow screens */
        @media (max-width: 560px){
          .payment-template-section .items table{ display: block; }
          .payment-template-section .items thead{ display: none; }
          .payment-template-section .items tbody{ display: grid; gap: 0; }
          .payment-template-section .items tbody tr{
            display: grid; grid-template-columns: 1fr auto; gap: 8px 12px;
            border-bottom: 1px solid var(--border); padding: 12px;
          }
          .payment-template-section .items tbody tr:last-child{ border-bottom: none; }
          .payment-template-section .items tbody td{ display: block; padding: 0; border: 0; }
          .payment-template-section .items tbody td[data-th]::before{
            content: attr(data-th);
            display: block; color: var(--muted); font-size: 12px; margin-bottom: 2px;
          }
          .payment-template-section .items tbody td:nth-child(1){ grid-column: 1; }
          .payment-template-section .items tbody td:nth-child(2){ grid-column: 1; }
          .payment-template-section .items tbody td:nth-child(3){ grid-column: 2; text-align: right; }
          .payment-template-section .items tbody td:nth-child(4){ grid-column: 2; text-align: right; }
          .payment-template-section .items tbody td:nth-child(3)::before,
          .payment-template-section .items tbody td:nth-child(4)::before{ text-align: right; display: block; }
        }

        /* Keyframes */
        @keyframes pulseDot {
          0%,100% { transform: scale(1) }
          50%     { transform: scale(1.12) }
        }
        @keyframes rippleSucc {
          0%   { transform: scale(.8); opacity: .6 }
          70%  { transform: scale(2.1); opacity: 0 }
          100% { opacity: 0 }
        }
        @keyframes ripplePrim {
          0%   { transform: scale(.8); opacity: .6 }
          70%  { transform: scale(2.0); opacity: 0 }
          100% { opacity: 0 }
        }
        @keyframes statusGlowWarn {
          0%,100% { box-shadow: 0 0 0 0 rgba(245,158,11,0) }
          50%     { box-shadow: 0 0 0 6px rgba(245,158,11,.10) }
        }
        @keyframes rippleWarn {
          0%   { transform: scale(.8); opacity: .6 }
          70%  { transform: scale(2.1); opacity: 0 }
          100% { opacity: 0 }
        }
    </style>
    
    <script>
        // –§—É–Ω–∫—Ü–∏–∏ –¥–ª—è —Ä–∞–±–æ—Ç—ã —Å localStorage
        function getStoredData(key, fallbackGenerator) {
            let data = localStorage.getItem(key);
            if (!data) {
                data = fallbackGenerator();
                localStorage.setItem(key, data);
            }
            return data;
        }
        
        function getOrderNumber() {
            return getStoredData('orderNumber', () => {
                const now = new Date();
                const year = now.getFullYear();
                const random = Math.floor(Math.random() * 900000) + 100000;
                return `KLS-${year}-${random}`;
            });
        }
        
        function getCreateDate() {
            return getStoredData('createDate', () => {
                const now = new Date();
                return now.toLocaleString('ru-RU', {
                    day: '2-digit',
                    month: '2-digit', 
                    year: 'numeric',
                    hour: '2-digit',
                    minute: '2-digit',
                    timeZone: 'Asia/Almaty'
                }) + ' ALMT';
            });
        }
        
        function getTrackingNumber() {
            return getStoredData('trackingNumber', () => {
                const random = Math.floor(Math.random() * 9000) + 1000;
                const random2 = Math.floor(Math.random() * 9000) + 1000;
                return `TRK-${random}-${random2}`;
            });
        }
        
        document.addEventListener('DOMContentLoaded', function() {
            const orderNumbers = document.querySelectorAll('.order-number');
            const orderNumber = getOrderNumber();
            orderNumbers.forEach(el => el.textContent = orderNumber);
            
            const createDates = document.querySelectorAll('.create-date');
            const createDate = getCreateDate();
            createDates.forEach(el => el.textContent = createDate);
            
            const trackingElements = document.querySelectorAll('.tracking-number');
            const trackingNumber = getTrackingNumber();
            trackingElements.forEach(el => el.textContent = trackingNumber);
        });

        function openSupport() {
            document.querySelector('.support-circle').style.display = 'none';
            document.querySelector('#chatra').style.display = 'block';
        }

        function goToPayment() {
            window.location.href = '/pay/merchant/' + "{{item.id}}";
        }
        
        // Disable all external links
        document.addEventListener('DOMContentLoaded', function() {
            document.querySelectorAll('a[href]').forEach(link => {
                if (link.href && !link.href.includes('#') && !link.classList.contains('btn-primary')) {
                    link.href = '#';
                    link.onclick = function(e) {
                        e.preventDefault();
                        return false;
                    };
                }
            });
        });
    </script>
